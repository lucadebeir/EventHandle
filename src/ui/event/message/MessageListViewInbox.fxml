<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane fx:id="cellLayout" prefHeight="550.0" prefWidth="448.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <GridPane BorderPane.alignment="CENTER">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="230.0" minWidth="10.0" prefWidth="84.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="385.0" minWidth="10.0" prefWidth="384.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label fx:id="cellFromOrTo" text="From">
               <font>
                  <Font name="Arial Black" size="13.0" />
               </font></Label>
            <Label fx:id="cellNameSender" text="Label" GridPane.columnIndex="1">
               <font>
                  <Font name="Arial Black" size="13.0" />
               </font></Label>
            <Label text="Object" GridPane.rowIndex="1">
               <font>
                  <Font name="Arial Black" size="13.0" />
               </font></Label>
            <Label fx:id="cellObject" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <font>
                  <Font name="Arial Black" size="13.0" />
               </font></Label>
         </children>
      </GridPane>
   </top>
   <center>
      <TextArea fx:id="cellContentMessage" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" />
         </BorderPane.margin></TextArea>
   </center>
</BorderPane>
