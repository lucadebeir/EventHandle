<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>


<BorderPane fx:id="cellLayout" prefWidth="850.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8">
   <center>
      <TextArea fx:id="chatContent" prefHeight="100.0" prefWidth="850.0" wrapText="true" BorderPane.alignment="CENTER">
         <font>
            <Font name="Arial Black" size="13.0" />
         </font>
      </TextArea>
   </center>
   <left>
      <Label fx:id="cellNameSender" prefHeight="17.0" prefWidth="97.0" text="Label" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets left="15.0" />
         </BorderPane.margin>
         <font>
            <Font name="Arial Black" size="20.0" />
         </font>
      </Label>
   </left>
</BorderPane>
